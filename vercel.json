{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "childs_right_act.pdf",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server.js"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "PORT": "3001"
  }
} 